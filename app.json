{
  "expo": {
    "name": "pizza-app",
    "slug": "pizza-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/pizza.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/img/splashScreen.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "userInterfaceStyle": "automatic",
      "supportsTablet": true
    },
    "android": {
      "userInterfaceStyle": "automatic",
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/pizza.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/img/pizza.png"
    }
  }
}
